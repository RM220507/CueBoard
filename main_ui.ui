<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="customtkinter.CTk" id="ctk1">
    <property name="title" translatable="yes">Cue Board</property>
    <child>
      <object class="customtkinter.CTkTabview" id="ctktabview1">
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="customtkinter.CTkTabview.Tab" id="show_tab" named="True">
            <property name="label" translatable="yes">Show</property>
            <containerlayout manager="grid">
              <property type="col" id="1" name="weight">3</property>
              <property type="col" id="2" name="weight">2</property>
              <property type="row" id="0" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="customtkinter.CTkFrame" id="quick_command_frame" named="True">
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">5</property>
                  <property name="pady">5</property>
                  <property name="row">0</property>
                  <property name="sticky">nsew</property>
                </layout>
                <child>
                  <object class="customtkinter.CTkButton" id="go_btn" named="True">
                    <property name="command" type="command" cbtype="simple">cue_go</property>
                    <property name="font">{Arial Black} 36 {bold}</property>
                    <property name="height">100</property>
                    <property name="text" translatable="yes">GO</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkFrame" id="ctkframe2">
                    <layout manager="pack">
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="customtkinter.CTkButton" id="ctkbutton3">
                        <property name="command" type="command" cbtype="simple">mute</property>
                        <property name="text" translatable="yes">Mute</property>
                        <property name="width">75</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkButton" id="ctkbutton4">
                        <property name="command" type="command" cbtype="simple">stop_all</property>
                        <property name="text" translatable="yes">Stop</property>
                        <property name="width">75</property>
                        <layout manager="grid">
                          <property name="column">1</property>
                          <property name="row">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkButton" id="ctkbutton5">
                        <property name="command" type="command" cbtype="simple">fade_out</property>
                        <property name="text" translatable="yes">Fade Out</property>
                        <property name="width">75</property>
                        <layout manager="grid">
                          <property name="column">2</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkSlider" id="volume_slider" named="True">
                        <property name="command" type="command" cbtype="simple">update_volume</property>
                        <property name="from_">0</property>
                        <property name="number_of_steps">100</property>
                        <property name="progress_color">#e01f8e</property>
                        <property name="to">100</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="columnspan">3</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">1</property>
                          <property name="sticky">nsew</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkButton" id="ctkbutton6">
                        <property name="command" type="command" cbtype="simple">restart_show</property>
                        <property name="text" translatable="yes">Restart</property>
                        <property name="width">75</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">2</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkButton" id="ctkbutton7">
                        <property name="command" type="command" cbtype="simple">next_cue</property>
                        <property name="text" translatable="yes">Next Cue</property>
                        <property name="width">75</property>
                        <layout manager="grid">
                          <property name="column">1</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">2</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkButton" id="ctkbutton8">
                        <property name="command" type="command" cbtype="simple">jump_end</property>
                        <property name="text" translatable="yes">End Jump</property>
                        <property name="width">75</property>
                        <layout manager="grid">
                          <property name="column">2</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">2</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkFrame" id="ctkframe3">
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="customtkinter.CTkLabel" id="cue_label" named="True">
                        <property name="text" translatable="yes">Next Cue:</property>
                        <layout manager="pack">
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkSlider" id="cue_select_slider" named="True">
                        <property name="command" type="command" cbtype="simple">jump_cue</property>
                        <property name="from_">0</property>
                        <property name="progress_color">green</property>
                        <layout manager="pack">
                          <property name="fill">x</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkLabel" id="clock_label" named="True">
                    <property name="font">{Arial} 48 {}</property>
                    <property name="text" translatable="yes">00:00:00.0</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkButton" id="load_show_btn" named="True">
                    <property name="command" type="command" cbtype="simple">load_show_prompt</property>
                    <property name="text" translatable="yes">Load Show</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="customtkinter.CTkTabview.Tab" id="build_tab" named="True">
            <property name="label" translatable="yes">Build</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
